program Compare

function compare(x, y) as int
  if x > y then
    if x > 0 then
        return 1
    else
        return 2
    end if
  else if x < y then
    return -1
  else
    return 0 
  end if
end function

int cmp = compare(-1,-2)
println("compare of -1 to -2 = " + cmp)

cmp = compare(1,2)
println("compare of 1 to 2 = " + cmp)

cmp = compare(2,1)
println("compare of 2 to 1 = " + cmp)
